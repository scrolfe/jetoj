<h1>Events#show</h1>

<h2><%= @event.name %></h2>
<p><%= image_tag @event.image.url %></p>
<p><%= @event.date_time %></p>
<p>
  <%= @event.address['street_address'] %> <%= @event.address['city'] %>, <%= @event.address['state'] %> <%= @event.address['zip'][0,5] %>
</p>
<p><%= @event.description %></p>
