<h1>Media#new</h1>

<%= form_for @medium do |f| %>
  <%= f.label :image %><br />
  <%= f.file_field :image %><br />
  <%= f.label :youtube_id %><br />
  <%= f.text_field :youtube_id %><br />
  <%= f.label :description %><br />
  <%= f.text_area :description %><br />
  <%= f.submit 'Create' %>
<% end %>
